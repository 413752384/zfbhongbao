     36%{ -webkit-transform: translateY(-192px); }
     48%{ -webkit-transform: translateY(-256px); }
     61% { -webkit-transform: translateY(-320px); }
     74% { -webkit-transform: translateY(-384px); }
     86% { -webkit-transform: translateY(-448px); }
     100% { -webkit-transform: translateY(-512px); }
 }
 @keyframes scrollText1 {
  0%{ transform: translateY(0); } 
  12%{ transform: translateY(-64px); } 
  24%{ transform: translateY(-128px); } 
  36%{ transform: translateY(-192px); } 
  48%{ transform: translateY(-256px); } 
  61% { transform: translateY(-320px); } 
  74% { transform: translateY(-384px); } 
  86% { transform: translateY(-448px); } 
  100% { transform: translateY(-512px); } 
} 
.scrolling-animate{ -webkit-animation:scrollText1 5.4s infinite cubic-bezier(1,0,0.5,0); animation: scrollText1 5.4s infinite cubic-bezier(1,0,0.5,0); }

.fbtn-group .btn{padding:6px 10px;}</style>
